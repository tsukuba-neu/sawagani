    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.28.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "simple-git-hooks": "2.11.1",
    "typescript-eslint": "8.8.0",
    "vite": "5.4.8",
    "vue-tsc": "2.1.6"
